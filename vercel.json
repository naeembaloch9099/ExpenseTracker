{
  "version": 2,
  "builds": [
    {
      "src": "BackEnd/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "FrontEnd/dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "BackEnd/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/FrontEnd/dist/$1"
    }
  ]
}
